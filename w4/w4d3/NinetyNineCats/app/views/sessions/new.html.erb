<h1>Login</h1>
<%= render "shared/errors" %>
<form action="<%= session_url %>" method="post">
  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
  <label for="user[user_name]">User Name:</label><br>
  <input id="user[user_name]" type="text" name="user[user_name]", value= "<%=@user.user_name %>" />
  <br>
  <label for="user[password]">Password:</label><br>
  <input id="user[password]" type="password" name="user[password]", value= "<%=@user.password %>" />

  <br>

  <input type="submit" value="Submit!" />
</form>
